{% extends "base.html.twig" %}


{% block body %}

    <div class="container">
       
        <div class="div-center">
            <div class="security-forms">
                <h2>Inscription</h2>
                {{ form_start(form) }}
                    {{ form_row(form.nom) }}
                    {{ form_row(form.prenom) }}
                    {{ form_row(form.username) }}
                    {{ form_row(form.password) }}
                    {{ form_row(form.email) }}
                    <br/>
                    <button type="submit" class="btn btn-success">s'inscrire</button>
                {{ form_end(form) }}
            </div>
        </div>
    </div>

{% endblock %}